<%= form_for [@league, @team] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :founded %>
  <%= f.date_select :founded, :start_year => 1850, :end_year => Date.today.year %>
  <br>
  <%= f.label :ground %>
  <%= f.text_field :ground %>
  <br>
  <%= f.label :coach %>
  <%= f.text_field :coach %>
  <br>
  <%= f.submit %>
<% end %>